{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src":[
        {
          "files": [
            "UnityDocs.csproj"
          ]
        }
      ], 
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site1",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "CachingGameForKids",
      "_appTitle": "CachingGameForKids",
      "_enableSearch": true,
      "pdf": true
    }
  }
}